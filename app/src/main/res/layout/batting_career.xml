<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="@dimen/_20sdp"
    android:elevation="@dimen/_3sdp"
    android:background="@color/card_background"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="t20"
        android:textStyle="bold"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/card_background"
        app:layout_constraintTop_toBottomOf="@id/label_type"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_matches"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="Matches"
        android:textSize="16sp"
        android:layout_marginStart="@dimen/_10sdp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/label_type" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_runs"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:text="Runs"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/matches" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_innings"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="Innings"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/runs1" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_balls"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="Balls"
        android:layout_marginStart="@dimen/_10sdp"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/innings" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_high_score"
        android:layout_width="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="High Score"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balls1" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_100s"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:text="100's"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/high_score" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_50s"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="50's"
        android:layout_marginStart="@dimen/_10sdp"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/s100" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_average"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="Average"
        android:layout_marginStart="@dimen/_10sdp"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/s50" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_not_out"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="Not Out"
        android:layout_marginStart="@dimen/_10sdp"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/average" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_4s"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:text="4's"
        android:layout_marginStart="@dimen/_10sdp"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/not_out" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_6s"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:text="6's"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/s4" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/label_strike_rate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:text="Strike Rate"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/label_high_score"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/s6" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/matches"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="232"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_matches" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/runs1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Australia vs India"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_runs" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/innings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="1st T20I"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_innings" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/balls1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Australia"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_balls" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/high_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="India"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_high_score" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/s100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Thu, Feb 09"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_100s" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/s50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="10.00 AM, Your Time"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_50s" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/average"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Finished"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_average" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/not_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="India won by 20 runs"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_not_out" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/s4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Australia opt to bat"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_4s" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/s6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Australia opt to bat"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_6s" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/strike_rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10sdp"
        android:padding="@dimen/_5sdp"
        android:text="Australia opt to bat"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@id/label_high_score"
        app:layout_constraintTop_toTopOf="@id/label_strike_rate" />

</androidx.constraintlayout.widget.ConstraintLayout>